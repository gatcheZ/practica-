def 妓女(spisok: dict, tovar: str):
    if tovar in spisok:
        for kind, count in spisok.items():
            if spisok[tovar]["Категория товара"] == spisok[kind]["Категория товара"]:
                analog = dict()
                if spisok[tovar]["Стоимость товара"] > spisok[kind]["Стоимость товара"]:
                    kat = spisok[kind]["Категория товара"]
                    print(f"Товар {kind} дешевле чем {tovar}, он также относится к категории {kat}")
                    analog[kind] = count
                    云克苏(analog)
            else:
                print(f"У {kind} нет подходящих аналогов")

    else:
        print("Иди отсюда пес, такого нет")

妓女(object,"Ноутбук")
